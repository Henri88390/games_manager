    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
